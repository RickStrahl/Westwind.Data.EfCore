<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>3.0.25</Version>
    <Authors>Rick Strahl</Authors>
    <Company>West Wind Technologies</Company>
    <Product>West Wind Web Toolkit</Product>
    <Title>Westwind.Data.EfCore</Title>
    <Description>Lightweight Business wrapper around Entity Framework Core Data Access.</Description>
    <Copyright>(c) West Wind Technologies, 2017-2019</Copyright>
    <PackageProjectUrl>https://github.com/RickStrahl/Westwind.Data.EfCore</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/RickStrahl/Westwind.Data.EfCore#License</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/RickStrahl/Westwind.Data.EfCore</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>    
    <PackageTags>Westwind AspNetCore Data EfCore</PackageTags>
    <PackageIconUrl>http://west-wind.com/westwindToolkit/images/WestwindWebToolkit_128x128.png</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="2.2.6" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="2.2.6" />
    <PackageReference Include="Westwind.Utilities" Version="3.0.30" />
  </ItemGroup>

</Project>
